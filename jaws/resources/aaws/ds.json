{
  "attributes": {
    "source": "surface observation",
    "featureType": "timeSeries",
    "institution": "UW SSEC",
    "reference": "https://amrc.ssec.wisc.edu/",
    "Conventions": "CF-1.7",
    "data_type": "q1h",
    "time_convention": "'time: point' variables are valid for exactly the time value stored in the time coordinate, whereas 'time: mean' variables are valid for the mean time within the time_bounds variable. For example, air_temp was continuously measured at high frequency and then averaged over each period contained in the time_bounds variable into the store hourly-mean values."
  },
  "variables": {
    "ta": {
      "type": "float",
      "attributes": {
        "long_name": "Air Temperature",
        "standard_name": "air_temperature",
        "original_name": "air_temp",
        "units": "kelvin",
        "cell_methods": "time: mean",
        "coordinates": "longitude latitude"
      }
    },
    "vtempdiff": {
      "type": "float",
      "attributes": {
        "long_name": "Vertical Temperature Differential",
        "original_name": "vtempdiff",
        "units": "1",
        "cell_methods": "time: mean",
        "coordinates": "longitude latitude"
      }
    },
    "rh": {
      "type": "float",
      "attributes": {
        "long_name": "Relative Humidity",
        "standard_name": "relative_humidity",
        "original_name": "rh",
        "units": "1",
        "cell_methods": "time: mean",
        "coordinates": "longitude latitude"
      }
    },
    "pa": {
      "type": "float",
      "attributes": {
        "long_name": "Air Pressure",
        "standard_name": "air_pressure",
        "original_name": "pressure",
        "units": "pascal",
        "cell_methods": "time: mean",
        "coordinates": "longitude latitude"
      }
    },
    "wdir": {
      "type": "float",
      "attributes": {
        "long_name": "Wind Direction",
        "standard_name": "wind_from_direction",
        "original_name": "wind_dir",
        "units": "degree",
        "cell_methods": "time: mean",
        "coordinates": "longitude latitude"
      }
    },
    "wspd": {
      "type": "float",
      "attributes": {
        "long_name": "Wind Speed",
        "standard_name": "wind_speed",
        "original_name": "wind_spd",
        "units": "meter second-1",
        "cell_methods": "time: mean",
        "coordinates": "longitude latitude"
      }
    },
    "time": {
      "type": "double",
      "attributes": {
        "long_name": "Time",
        "standard_name": "time",
        "units": "seconds since 1970-01-01 00:00:00",
        "bounds": "time_bounds",
        "calendar": "standard"
      }
    },
    "sza": {
      "type": "double",
      "attributes": {
        "long_name": "Solar Zenith Angle",
        "standard_name": "solar_zenith_angle",
        "units": "degree",
        "cell_methods": "time: mean",
        "coordinates": "longitude latitude"
      }
    },
    "station_name": {
      "type": "string",
      "attributes": {
        "long_name": "Station Name",
        "cf_role": "timeseries_id"
      }
    },
    "latitude": {
      "type": "double",
      "attributes": {
        "long_name": "Latitude",
        "standard_name": "latitude",
        "units": "degrees_north"
      }
    },
    "longitude": {
      "type": "double",
      "attributes": {
        "long_name": "Longitude",
        "standard_name": "longitude",
        "units": "degrees_east"
      }
    },
    "hour": {
      "type": "int",
      "attributes": {
        "long_name": "Hour of day(UTC)",
        "units": "1"
      }
    },
    "day": {
